{
  "name": "@pancakeswap/permit2-sdk",
  "version": "1.0.0",
  "description": "An sdk for interacting with permit2.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pancakeswap/pancake-frontend.git",
    "directory": "packages/permit2-sdk"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --globals --run"
  },
  "keywords": [
    "pancakeswap",
    "ethereum"
  ],
  "dependencies": {
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/providers": "^5.0.0",
    "@pancakeswap/sdk": "workspace:*",
    "ethers": "^5.3.1",
    "tiny-invariant": "^1.3.1",
    "tslib": "^2.6.2",
    "viem": "1.10.9"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "prettier": "^2.4.1",
    "tsup": "^6.7.0",
    "vitest": "^0.30.1"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  }
}