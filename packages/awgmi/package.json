    "./connectors/rise": {
      "types": "./dist/connectors/rise.d.ts",
      "import": "./dist/connectors/rise.mjs",
      "default": "./dist/connectors/rise.js"
    }
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "@blocto/sdk": {
      "optional": true
    }
  }
