    "@pancakeswap/tsconfig": "workspace:*",
    "@pancakeswap/utils": "workspace:*",
    "@tanstack/react-query": "^5.28.4",
    "@types/react": "^18.2.21",
    "@types/use-sync-external-store": "^0.0.3",
    "aptos": "1.21.0",
    "happy-dom": "^13.3.8",
    "tsup": "^6.7.0"
  },
  "files": [
    "dist",
    "connectors",
    "core/package.json"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "rm -rf dist && tsup",
    "test:u": "vitest -u --run",
