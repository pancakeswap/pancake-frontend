{
  "name": "@pancakeswap/smart-router",
  "version": "0.6.1",
  "description": "A SDK for getting best routes from AMM",
  "repository": {
    "type": "git",
    "url": "https://github.com/pancakeswap/pancake-frontend.git",
    "directory": "packages/smart-router"
  },
  "scripts": {
    "build": "yarn typechain && tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "typechain": "typechain --out-dir evm/abis/types --target=ethers-v5 \"evm/abis/*.json\"",
    "postinstall": "yarn typechain",
    "prepublishOnly": "yarn run build",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "peerDependencies": {
    "@pancakeswap/sdk": "^3.1.3",
    "@pancakeswap/v3-sdk": "*"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.0.0",
    "@ethersproject/contracts": "^5.0.0",
    "@ethersproject/providers": "^5.0.0",
    "@ethersproject/solidity": "^5.0.0",
    "@pancakeswap/multicall": "^0.0.2",
    "async-retry": "^1.3.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "lodash": "^4.17.21",
    "stats-lite": "^2.2.0",
    "tiny-invariant": "^1.1.0"
  },
  "devDependencies": {
    "@pancakeswap/sdk": "^3.1.3",
    "@pancakeswap/tokens": "*",
    "@pancakeswap/token-lists": "*",
    "@pancakeswap/v3-sdk": "*",
    "@typechain/ethers-v5": "^8.0.5",
    "@types/async-retry": "^1.4.4",
    "@types/lodash": "^4.14.178",
    "@types/stats-lite": "^2.2.0",
    "tsconfig": "*",
    "tsup": "^6.5.0",
    "typechain": "^6.1.0",
    "typescript": "^4.9.4"
  },
  "exports": {
    "./evm": {
      "types": "./dist/evm.d.ts",
      "import": "./dist/evm.mjs",
      "require": "./dist/evm.js"
    }
  }
}
